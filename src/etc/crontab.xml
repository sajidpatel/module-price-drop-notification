<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="mumzworld_process_price_drop_notification_queue" instance="MumzWorld\PriceDropNotification\Cron\ProcessPriceDropNotificationQueue" method="execute">
            <config_path>crontab/default/jobs/mumzworld_process_price_drop_notification_queue/schedule/cron_expr</config_path>
        </job>
        <job name="mumzworld_cleanup_price_drop_notifications" instance="MumzWorld\PriceDropNotification\Cron\CleanupNotifications" method="execute">
            <config_path>crontab/default/jobs/mumzworld_cleanup_price_drop_notifications/schedule/cron_expr</config_path>
        </job>
    </group>
</config>